                android:layout_marginLeft="100dp"
                android:text="- job 2"
                android:textColor="@color/amber"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/job3ERN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/job2ERN"
                android:layout_marginLeft="100dp"
                android:text="- job 3"
                android:textColor="@color/amber"
                android:textSize="20sp" />

            <ImageView
                android:layout_width="80dp"
                android:layout_height="100dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:src="@drawable/girl3" />

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:id="@+id/profANI"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_below="@id/profERN"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="@color/dark_blue"
        android:onClick="intentProfile"
        app:cardCornerRadius="20dp">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/nmANI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="20dp"
                android:text="Erny"
                android:textColor="@color/amber"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/job1ANI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/nmANI"
                android:layout_marginLeft="100dp"
                android:text="- job 1"
                android:textColor="@color/amber"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/job2ANI"
                android:layout_width="wrap_content"
